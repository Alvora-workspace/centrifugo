{
  "admin": {
    "enabled": true
  },
  "engine": {
    "type": "memory"
  },
  "log": {
    "level": "info"
  },
  "client": {
    "allowed_origins": ["http://localhost:3000"]
  },
  "websocket_compression": true,
  
  "namespaces": [
    {
      "name": "notifications",
      "publish": true,
      "subscribe": true,
      "subscribe_to_publish": false,
      "presence": false,
      "history_size": 10,
      "history_ttl": "300s"
    },
    {
      "name": "presence", 
      "publish": true,
      "subscribe": true,
      "subscribe_to_publish": false,
      "presence": true,
      "join_leave": true,
      "history_size": 1,
      "history_ttl": "60s"
    },
    {
      "name": "office_access",
      "publish": true,
      "subscribe": true,
      "subscribe_to_publish": false,
      "presence": false,
      "history_size": 5,
      "history_ttl": "120s"
    },
    {
      "name": "online_status",
      "publish": true,
      "subscribe": true,
      "subscribe_to_publish": false,
      "allow_subscribe_for_client": true,
      "presence": true,
      "join_leave": true,
      "history_size": 1,
      "history_ttl": "30s"
    }
  ]
}