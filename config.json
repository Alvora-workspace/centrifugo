{
  "admin": true,
  "admin_password": "$CENTRIFUGO_ADMIN_PASSWORD",
  "admin_secret": "$CENTRIFUGO_ADMIN_SECRET",
  "token_hmac_secret_key": "$CENTRIFUGO_TOKEN_SECRET",
  "api_key": "$CENTRIFUGO_API_KEY",
  
  "allowed_origins": "$CENTRIFUGO_ALLOWED_ORIGINS",
  
  "engine": "memory",
  
  "namespaces": [
    {
      "name": "notifications",
      "publish": true,
      "subscribe_to_publish": true,
      "presence": false,
      "history_size": 10,
      "history_ttl": "300s"
    },
    {
      "name": "presence", 
      "publish": true,
      "subscribe_to_publish": true,
      "presence": true,
      "join_leave": true,
      "history_size": 1,
      "history_ttl": "60s"
    },
    {
      "name": "office_access",
      "publish": true,
      "subscribe_to_publish": true,
      "presence": false,
      "history_size": 5,
      "history_ttl": "120s"
    },
    {
      "name": "online_status",
      "publish": true,
      "subscribe_to_publish": true,
      "presence": true,
      "join_leave": true,
      "history_size": 1,
      "history_ttl": "30s"
    }
  ],
  
  "log_level": "info"
}